#<<BM1 DOOR INTERLOCKS>>
action (BM1.DoorLXO == Close) {
	$Count = 3; $Interval = 1;
	condition (FM.Robot_Run_Status == Fault || TM.AtmRbRet_BM1DI != On) $alarm = 8321;
}

action (BM1.DoorRXO == Close) {
	$Count = 3; $Interval = 1;	
	condition (FM.Robot_Run_Status == Fault || TM.AtmRbRet_BM2DI != On) $alarm = 8321;
}

action (BM1.DoorLXO == Open) {
	$Count = 3; $Interval = 1;	
        condition (BM1.ATMVACSensorXI != ATM || BM1.RoughVvXO != Close ) $alarm = 8322;
}
action (BM1.DoorRXO == Open) {
	$Count = 3; $Interval = 1;
        condition (BM1.ATMVACSensorXI != ATM || BM1.RoughVvXO != Close ) $alarm = 8322;
}

#<<BM1 VALVE INTERLOCKS>>
action (BM1.VentVvXO == OpenFast) {
       condition (BM1.RoughVvXO != Close || BM1.DoorLXI != Close  || BM1.DoorRXI != Close) $alarm = 8323;
}

action (BM1.VentVvXO == OpenSlow) {
       condition (BM1.RoughVvXO != Close || BM1.DoorLXI != Close  || BM1.DoorRXI != Close) $alarm = 8324;
}

action (BM1.RoughVvXO == OpenFast) {
       condition (BM1.VentVvXO  != Close || BM1.DoorLXI != Close  || BM1.DoorRXI != Close) $alarm = 8325;
}

action (BM1.RoughVvXO == OpenSlow) {
       condition (BM1.VentVvXO  != Close || BM1.DoorLXI != Close  || BM1.DoorRXI != Close ) $alarm = 8326;
}


#<<ATM ROBOT PICK/PLACE INTERLOCKS>>
#Robot vs Motor Position
action (FM.Robot_Get_Robot == 6)  {
       condition (BM1.RoughVvXO != Close || BM1.DoorLXI != Open  || BM1.DoorRXI != Open || BM1.ATMVACSensorXI != ATM)   $alarm = 8301;
}

action (FM.Robot_Get_RobotB == 6) {
       condition (BM1.RoughVvXO != Close  || BM1.DoorLXI != Open  || BM1.DoorRXI != Open || BM1.ATMVACSensorXI != ATM)   $alarm = 8301;
}

