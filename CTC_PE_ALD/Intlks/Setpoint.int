###################################################################################################
# LPMA (Station 1)
###################################################################################################

#------------------------------------------------------------------
# Mapping & LPM Door Interlock
#------------------------------------------------------------------
#------------------------------------------------------------------
action (LPMA.Mapping == Map) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down || LPMA.WaferSlideOutDI != Off ) $alarm = 8001;
}
#------------------------------------------------------------------
action (FM.Robot_Get_Robot == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8002;
}
action (FM.Robot_Get_RobotB == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8002;
}
action (FM.Robot_GetDB_Robot == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8002;
}
#------------------------------------------------------------------
action (FM.Robot_Put_Robot == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8003;
}
action (FM.Robot_Put_RobotB == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8003;
}
action (FM.Robot_PutDB_Robot == 1) {#LPMA (CM1)
       condition (LPMA.DoorUpDownXI != Down) $alarm = 8003;
}

#------------------------------------------------------------------
# Undock Interlock
#------------------------------------------------------------------
action (LPMA.UndockDO == CloseDoor) {#LPMA (CM1)
       condition (FM.Robot_Run_Status == Fault || TM.AtmRbRet_LPMADI != On) $alarm = 8005;
}

action (LPMA.UndockDO == FullUndock) {#LPMA (CM1)
       condition ( FM.Robot_Run_Status == Fault || TM.AtmRbRet_LPMADI != On) $alarm = 8005;
}
#------------------------------------------------------------------
# Foup Placement Interlock
#------------------------------------------------------------------
action (FM.Robot_Get_Robot == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8006;
}
action (FM.Robot_Get_RobotB == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8006;
}
action (FM.Robot_GetDB_Robot == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8006;
}
#------------------------------------------------------------------
action (FM.Robot_Put_Robot == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8007;
}
action (FM.Robot_Put_RobotB == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8007;
}
action (FM.Robot_PutDB_Robot == 1) {#LPMA (CM1)
       condition (LPMA.F_ProperlyPlaceDI != On) $alarm = 8007;
}

#------------------------------------------------------------------
# Shuttle In Interlock
